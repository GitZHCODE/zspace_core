{
    "Plugins": [
        {
            "Info": {
                "Types": {
                    "OmniUsdResolver" : {
                        "bases": [ "ArResolver" ],
                        "implementsContexts": true,
                        "implementsScopedCaches" : true
                    },
                    "OmniUsdWrapperFileFormat": {
                        "bases": [ "SdfFileFormat" ],
                        "displayName": "Omniverse Wrapper File Format",
                        "extensions": [ "omni" ],
                        "formatId": "omni",
                        "primary": true,
                        "target": "usd"
                    },
                    "OmniUsdLiveFileFormat": {
                        "bases": [ "SdfFileFormat" ],
                        "displayName": "Omniverse Live file format",
